ABUSEIPDB_API_KEY=

# URLScan.io: Live URL analysis (optional)
# Get from: https://urlscan.io/user/profile/
URLSCAN_API_KEY=

# Configuration
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000
